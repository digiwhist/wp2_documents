{
  "#schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "date": {
      "type": "string",
      "title": "TenderDate",
      "pattern": "^\\d{4}-[01]\\d-[0-3]\\d$",
      "description": "Date YYYY.MM.DD"
    },
    "date-time": {
      "type": "string",
      "title": "TenderDateTime",
      "pattern": "^\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d",
      "description": "Date with time YYYY.MM.DDThh:mm:ss"
    },
    "supply-type": {
      "type": "string",
      "title": "Supply Type",
      "enum": [
        "SUPPLIES",
        "WORKS",
        "SERVICES",
        "OTHER"
      ]
    },
    "procedure-type": {
      "type": "string",
      "title": "Procedure Type",
      "enum": [
        "OPEN",
        "RESTRICTED",
        "NEGOTIATED",
        "COMPETITIVE_DIALOG",
        "NEGOTIATED_WITH_PUBLICATION",
        "NEGOTIATED_WITHOUT_PUBLICATION",
        "DESIGN_CONTEST",
        "MINITENDER",
        "DPS_PURCHASE",
        "OUTRIGHT_AWARD",
        "APPROACHING_BIDDERS",
        "PUBLIC_CONTEST",
        "SIMPLIFIED_BELOW_THE_THRESHOLD",
        "INOVATION_PARTNERSHIP",
        "CONCESSION",
        "OTHER"
      ]
    },
    "size-type": {
      "type": "string",
      "title": "Size Type",
      "enum": [
        "BELOW_THE_THRESHOLD",
        "ABOVE_THE_THRESHOLD",
        "SMALL_SCALE"
      ]
    },
    "selection-method": {
      "type": "string",
      "title": "Selection Method",
      "description": "method used for bids evaluation - one of the LOWEST_PRICE or MEAT (most economically advantageous tender), individual MEAT criteria are in awardCriteria",
      "enum": [
        "MEAT",
        "LOWEST_PRICE"
      ]
    },
    "buyer-type": {
      "type": "string",
      "title": "Buyer Type",
      "enum": [
        "NATIONAL_AUTHORITY",
        "NATIONAL_AGENCY",
        "REGIONAL_AUTHORITY",
        "REGIONAL_AGENCY",
        "PUBLIC_BODY",
        "EUROPEAN_AGENCY",
        "UTILITIES",
        "OTHER"
      ]
    },
    "form-type": {
      "type": "string",
      "title": "Form Type",
      "enum": [
        "CONTRACT_NOTICE",
        "CONTRACT_AWARD",
        "CONTRACT_CANCELLATION",
        "CONTRACT_IMPLEMENTATION",
        "NOTICE_ON_BUYER_PROFILE",
        "PRIOR_INFORMATION_NOTICE",
        "CONTRACT_UPDATE",
        "CONTRACT_AMENDMENT",
        "OTHER",
	"COMPILED"
      ],
      "distinct": true,
      "description": "undocumented form-type enumeration"
    },
    "indicator-type": {
      "type": "string",
      "title": "Indicator Type",
      "enum": [
        "INTEGRITY_SINGLE_BID",
        "INTEGRITY_CALL_FOR_TENDER_PUBLICATION",
        "INTEGRITY_ADVERTISEMENT_PERIOD",
        "INTEGRITY_PROCEDURE_TYPE",
        "INTEGRITY_DECISION_PERIOD",
        "INTEGRITY_TAX_HAVEN",
        "INTEGRITY_NEW_COMPANY",
        "ADMINISTRATIVE_CENTRALIZED_PROCUREMENT",
        "ADMINISTRATIVE_ELECTRONIC_AUCTION",
        "ADMINISTRATIVE_COVERED_BY_GPA",
        "ADMINISTRATIVE_FRAMEWORK_AGREEMENT",
        "ADMINISTRATIVE_ENGLISH_AS_FOREIGN_LANGUAGE",
        "ADMINISTRATIVE_NOTICE_AND_AWARD_DISCREPANCIES",
        "TRANSPARENCY_NUMBER_OF_KEY_MISSING_FIELDS",
        "TRANSPARENCY_AWARD_DATE_MISSING",
"TRANSPARENCY_BUYER_NAME_MISSING",
"TRANSPARENCY_PROC_METHOD_MISSING",
"TRANSPARENCY_BUYER_LOC_MISSING",
"TRANSPARENCY_BIDDER_ID_MISSING",
"TRANSPARENCY_BIDDER_NAME_MISSING",
"TRANSPARENCY_MARKET_MISSING",
"TRANSPARENCY_TITLE_MISSING",
"TRANSPARENCY_VALUE_MISSING",
"TRANSPARENCY_YEAR_MISSING",
"INTEGRITY_WINNER_CA_SHARE" 
      ]
    },
    "score-type": {
      "type": "string",
      "title": "Score Type",
      "description": "Type of indicator score",
      "enum": [
        "INTEGRITY",
        "ADMINISTRATIVE",
        "TRANSPARENCY",
        "TENDER"
      ]
    },
    "indicator-value": {
      "type": "number",
      "title": "Indicator Score",
      "min": 0,
      "max": 100
    },
    "indicator-status-type": {
      "type": "string",
      "title": "Indicator Status Type",
      "enum": [
        "CALCULATED",
        "INSUFFICIENT_DATA",
        "UNDEFINED"
      ]
    },
    "language": {
      "type": "string",
      "title": "Language",
      "description": "undocumented language enumeration",
      "distinct": true
    },
    "cpv": {
      "type": "object",
      "additionalProperties": true,
      "title": "CPV",
      "properties": {
        "code": {
          "type": "string",
          "description": "CPV code of the subject"
        },
        "isMain": {
          "type": "boolean",
          "description": "Is main cpv code of the subject"
        },
        "name": {
          "type": "string",
          "description": "Localized name of cpv"
        }
      }
    },
    "award-criteria": {
      "type": "object",
      "additionalProperties": true,
      "title": "Award Criterion",
      "properties": {
        "name": {
          "type": "string",
          "description": "Name of criterion, PRICE if lowest price is used"
        },
        "weight": {
          "type": "number",
          "description": "Weight of criterion (scale 0-100)"
        },
        "description": {
          "type": "string",
          "description": "Broader description of the criterion"
        },
        "isPriceRelated": {
          "type": "boolean",
          "description": "Is the criterion directly related to price (monetary measurable)? such as price, lifetime costs, interest etc."
        }
      }
    },
    "funding": {
      "type": "object",
      "additionalProperties": true,
      "title": "Funding",
      "properties": {
        "programme": {
          "type": "string",
          "description": "Refined classification of sources (such as type of Operational programme etc..)"
        },
        "source": {
          "type": "string",
          "description": "source"
        },
        "isEuFund": {
          "type": "boolean",
          "description": "Is the source from EU funds?"
        }
      }
    },
    "unit-price": {
      "type": "object",
      "additionalProperties": true,
      "title": "Unit Price",
      "properties": {
        "unitNumber": {
          "type": "number",
          "description": "Number of units"
        },
        "currency": {
          "type": "string",
          "description": "currency"
        }
      }
    },
    "price": {
      "type": "object",
      "additionalProperties": true,
      "title": "Price",
      "properties": {
        "currency": {
          "type": "string",
          "description": "ISO 4217 of used currency",
          "distinct": true
        },
        "publicationDate": {
          "$ref": "#/definitions/date",
          "description": ""
        },
        "currencyNational": {
          "type": "string",
          "description": "ISO 4217 of used currency",
          "distinct": true
        },
        "vat": {
          "type": "number",
          "description": "VAT percentage (0 if no VAT is paid)"
        },
        "netAmount": {
          "type": "number",
          "description": "Price without VAT"
        },
        "netAmountNational": {
          "type": "number",
          "description": "Price in national currency without VAT"
        },
        "netAmountEur": {
          "type": "number",
          "description": "Price in EUR without VAT"
        },
        "amountWithVat": {
          "type": "number",
          "description": "Price including VAT"
        },
        "minNetAmount": {
          "type": "number",
          "description": "Minimum reachable value of netAmount - typically given by range estimate or spread of bids"
        },
        "maxNetAmount": {
          "type": "number",
          "description": "Maximum reachable value of netAmount - typically given by range estimate or spread of bids"
        },
        "minAmountWithVat": {
          "type": "number",
          "description": "Minimum reachable value of amountWithVAT"
        },
        "maxAmountWithVat": {
          "type": "number",
          "description": "Maximum reachable value of amountWithVAT"
        }
      }
    },
    "correction": {
      "type": "object",
      "additionalProperties": true,
      "title": "Correction",
      "properties": {
        "sectionNumber": {
          "type": "string",
          "description": "sectionNumber"
        },
        "lotNumber": {
          "type": "integer",
          "description": "lotNumber"
        },
        "placeOfModifiedText": {
          "type": "string",
          "description": "placeOfModifiedText"
        },
        "original": {
          "type": "string",
          "description": "original"
        },
        "replacement": {
          "type": "string",
          "description": "replacement"
        },
        "originalDate": {
          "type": "string",
          "description": "originalDate"
        },
        "replacementDate": {
          "type": "string",
          "description": "replacementDate"
        },
        "originalCpvs": {
          "description": "originalCpvs",
          "type": "array",
          "items": {
            "$ref": "#/definitions/cpv",
            "description": "cpv code of the subject"
          }
        },
        "replacementCpvs": {
          "description": "replacementCpvs",
          "type": "array",
          "items": {
            "$ref": "#/definitions/cpv",
            "description": "cpv code of the subject"
          }
        },
        "originalValue": {
          "type": "object",
          "additionalProperties": true,
          "description": "originalValue",
          "properties": {
            "currency": {
              "type": "string",
              "description": "Document url"
            },
            "netAmount": {
              "type": "number",
              "description": "netAmount"
            }
          }
        },
        "replacementValue": {
          "type": "object",
          "additionalProperties": true,
          "description": "replacementValue",
          "properties": {
            "currency": {
              "type": "string",
              "description": "Document url"
            },
            "netAmount": {
              "type": "number",
              "description": "netAmount"
            }
          }
        }
      }
    },
    "address": {
      "type": "object",
      "additionalProperties": true,
      "title": "Address",
      "properties": {
        "rawAddress": {
          "type": "string",
          "description": "Raw string for unstructured address"
        },
        "street": {
          "type": "string",
          "description": "Street including numbers"
        },
        "city": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "postcode": {
          "type": "string"
        },
        "country": {
          "type": "string",
          "description": "Country Code: ISO 3166-1 alpha-2 = two letter"
        },
        "url": {
          "type": "string",
          "description": "URL"
        },
        "nuts": {
          "type": "array",
          "description": "Array of Nuts",
          "items": {
            "type": [
              "string",
              "null"
            ],
            "description": "Original Nuts code of region"
          }
        },
        "ot": {
          "type": "object",
          "additionalProperties": true,
          "title": "Opentender Additional Information",
          "properties": {
            "nutscode": {
              "type": "string",
              "description": "Cleaned Nuts code"
            }
          }
        }
      }
    },
    "publication": {
      "type": "object",
      "additionalProperties": true,
      "title": "Publication",
      "properties": {
        "sourceId": {
          "type": "string",
          "description": "Identifier of publication on source"
        },
        "source": {
          "type": "string",
          "description": "source of publication"
        },
        "humanReadableUrl": {
          "type": "string",
          "description": "Url of tender on source, where human readable data are present, null if not available"
        },
        "machineReadableUrl": {
          "type": "string",
          "description": "Url of tender on source, where machine readable data are present, null if not available"
        },
        "buyerAssignedId": {
          "type": "string",
          "description": "Buyers custom identifier of publication"
        },
        "sourceTenderId": {
          "type": "string",
          "description": "Identifier of tender on source"
        },
        "publicationDate": {
          "$ref": "#/definitions/date",
          "description": "Date of publication (of given version)"
        },
        "dispatchDate": {
          "$ref": "#/definitions/date",
          "description": "Date of dispatch (different from publication date)"
        },
        "lastUpdate": {
          "$ref": "#/definitions/date"
        },
        "language": {
          "$ref": "#/definitions/language",
          "description": "Language code of the publication"
        },
        "formType": {
          "$ref": "#/definitions/form-type",
          "description": "Identifies given type of publication http://ocds.open-contracting.org/standard/r/1__0__RC/en/schema/codelists/#release-tag"
        },
        "sourceFormType": {
          "type": "string",
          "description": "Source specific type of form"
        },
        "isIncluded": {
          "type": "boolean",
          "description": "Are the refered publication data already merged into this publication package?"
        },
        "isValid": {
          "type": "boolean",
          "description": "isValid"
        },
        "isParentTender": {
          "type": "boolean",
          "description": "isParentTender"
        },
        "version": {
          "type": "number"
        }
      }
    },
    "document": {
      "type": "object",
      "additionalProperties": true,
      "title": "TenderDocument",
      "properties": {
        "title": {
          "type": "string"
        },
        "language": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "description": "Document url"
        },
        "publicationDateTime": {
          "$ref": "#/definitions/date-time"
        },
        "type": {
          "type": "string",
          "enum": [
            "CONTRACTOR_AGREEMENT"
          ]
        },
        "version": {
          "type": "number"
        }
      }
    },
    "body-metadata": {
